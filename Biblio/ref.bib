@article{Steglich1979,
abstract = {A comparison was made between four low-temperature properties of LaCu2Si2 and CeCu2Si2. Whereas LaCu2Si2 behaves like a normal metal, CeCu2Si2 shows (i) low-temperature anomalies typical of "unstable 4f shell" behavior and (ii) a transition into a superconducting state at Tc{\^{a}} 0.5 K. Our experiments demonstrate for the first time that superconductivity can exist in a metal in which many-body interactions, probably magnetic in origin, have strongly renormalized the properties of the conduction-electron gas. {\textcopyright} 1979 The American Physical Society.},
author = {Steglich,F. and Aarts, J. and Bredl, C. D. and Lieke, W. and Meschede, D. and Franz, W. and Sch{\"{a}}fer, H.},
journal = {Physical Review Letters},
number = {25},
pages = {1892-1896},
title = {{Superconductivity in the Presence of Strong Pauli Paramagnetism: \ce{CeCu2Si2}}},
volume = {43},
year = {1979}
}
@article{Lohneysen1994,
abstract = {The specific heat C and electrical resistivity $\rho$ of the heavy-fermion alloy CeCu5.9Au0.1 exhibit non-Fermi-liquid behavior well below 1 K, i.e., C/T-ln(T/T0) and $\rho$=$\rho$0+A'T, over more than a decade in temperature T. The magnetic susceptibility $\chi$ measured in 0.1 T shows a cusp for T→0. This behavior is attributed to the proximity to magnetic order: In contrast to CeCu6, CeCu6-xAux alloys show long-range antiferromagnetic order, with TN→0 for xc=0.1. Hence CeCu5.9Au0.1 is at the edge of a zero-temperature quantum phase transition. In a large magnetic field (B3 T) Fermi-liquid behavior is recovered. {\textcopyright} 1994 The American Physical Society.},
author = {L{\"{o}}hneysen, H. v. and Pietrus, T. and Portisch, G and Schlager, H G and Schr{\"{o}}der, A. and Sieck, M and Trappmann, T.},
journal = {Physical Review Letters},
number = {20},
pages = {3262-3265},
title = {{Non-Fermi-Liquid Behavior in a Heavy-Fermion Alloy at a Magnetic Instability}},
volume = {72},
year = {1994}
}
@misc{Mathur1998,
abstract = {In conventional superconductor, the binding of electrons into the paired states that collectively carry the supercurrent is mediated by phonons-vibrations of the crystal lattice. Here we argue that, in the case of the heavy fermion superconductors CePd2Si2 and CeIn3, the charge carriers are bound together in pairs by magnetic spin-spin interactions. The existence of magnetically mediated superconductivity in these compounds could help shed light on the question of whether magnetic interactions are relevant for describing the superconducting and normal-state properties of other strongly correlated electron systems, perhaps including the high-temperature copper oxide superconductors.},
author = {Mathur, N D and Grosche, F M and Julian, S R and Walker, I R and Freye, D M and Haselwimmer, R K W and Lonzarich, G G},
booktitle = {Nature},
number = {6688},
pages = {39-43},
title = {{Magnetically mediated superconductivity in heavy fermion compounds}},
volume = {394},
year = {1998}
}
