@article{Kondo1964,
abstract = {Based on the s-d interaction model for dilute magnetic alloys we have calculated the scattering probability of the conduction electrons to the second Born approximation. Because of the dynamical character of the localized spin system, the Pauli principle should be taken into account in the intermediate states of the second order terms. Thus the effect of the Fermi sphere is involved in the scattering probability and gives rise to a singular term in the resistivity which involves c log T as a factor, where c is the concentration of impurity atoms. When combined with the lattice resistivity, this gives rise to a resistance minimum, provided the s-d exchange integral J is negative. The temperature at which the minimum ccurs is proportional to c1/5 and the depth of the minimum to c, as is observed. The predicted log T dependence is tested with available experiments and is confirmed. The value of J to have fit with experiments is about -0.2 ev, which is of reasonable magnitude. Our conclusion is that J should be negative in alloys which show a resistance minimum. It is argued that the resistance minimum is a result of the sharp Fermi surface.},
author = {Kondo, J.},
journal = {Progress of Theoretical Physics},
number = {1},
pages = {37-49},
title = {{Resistance Minimum in Dilute Magnetic Alloys}},
volume = {32},
year = {1964}
}
@article{Sarachik1964,
abstract = {The resistivity of a series of Mo-Nb and Mo-Re alloys, with and without 1{\%} Fe, has been measured at room temperature, and between 1.5 and 77°K. Large effects are observed near the alloy composition where the iron acquires a localized magnetic moment. These effects appear both as an excess temperature-independent scattering and in the form of large anomalies at low temperatures. Interpreted in the light of current theories of localized moments, the resistivity results confirm the existence of virtual bound states near the Fermi level. In addition, the anomalous behavior of the resistivity at low temperatures has been directly related to the existence of a localized magnetic moment. {\textcopyright} 1964 The American Physical Society.},
author = {Sarachik, M. P. and Corenzwit, E. and Longinotti, L. D.},
journal = {Physical Review},
number = {4A},
title = {{Resistivity of Mo-Nb and Mo-Re Alloys Containing 1{\%} Fe}},
volume = {135},
pages={A1041-A1045},
year = {1964}
}
@article{Rech2006,
abstract = {We apply the Schwinger boson scheme to the fully screened Kondo model and generalize the method to include antiferromagnetic interactions between ions. Our approach captures the Kondo crossover from local moment behavior to a Fermi liquid with a nontrivial Wilson ratio. When applied to the two-impurity model, the mean-field theory describes the {\^{a}}€{\oe}Varma-Jones{\^{a}}€ quantum phase transition between a valence bond state and a heavy Fermi liquid. {\textcopyright} 2006 The American Physical Society.},
journal = {Physical Review Letters},
author={Rech, J. and Coleman, P. and Zarand, G. and Parcollet, J.},
number = {1},
pages = {016601},
title = {{Schwinger Boson Approach to the Fully Screened Kondo Model}},
volume = {96},
year = {2006}
}
